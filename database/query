/* TRIGGERS PARA VISUAL STUDIO */

--Si la venta fue realizada con una promoción, le descuenta al total el precio de una entrada
CREATE TRIGGER TR_ActualizaTotalPromo
on Ventas
after UPDATE
AS 
BEGIN 
set nocount on;
IF ((SELECT Estado_Venta FROM Ventas WHERE ID_Venta=(SELECT MAX(ID_Venta) FROM Ventas)) = 'Realizada')
BEGIN
IF((SELECT Precio FROM DetalleVentas WHERE ID_Venta_DV=(SELECT MAX(ID_Venta) FROM Ventas) AND ID_DetalleVenta=2) = 0.00 )
BEGIN
UPDATE Ventas
SET Total=Total-(SELECT Precio FROM DetalleVentas where ID_Venta_DV=(SELECT MAX(ID_Venta) FROM Ventas) AND ID_DetalleVenta=1)
WHERE ID_Venta= (SELECT MAX(ID_Venta) FROM Ventas)
END
END
END
GO 

--Actualiza el estado de detalle de venta depende si la venta se realizó o canceló
CREATE TRIGGER TR_ActualizarEstadoDV
ON Ventas
AFTER UPDATE
AS
BEGIN
set nocount on;
IF (SELECT Estado_Venta FROM inserted) = 'Realizada'
	BEGIN
	UPDATE DetalleVentas
	SET Estado_DV = 'Realizado'
	WHERE ID_Venta_DV = (SELECT ID_Venta FROM inserted);
    DBCC CHECKIDENT (DetalleVentas, RESEED,0);
	END
ELSE IF (SELECT Estado_Venta FROM inserted) = 'Cancelada'
	BEGIN
	UPDATE DetalleVentas
	SET Estado_DV = 'Cancelado'
	WHERE ID_Venta_DV = (SELECT ID_Venta FROM inserted);
	DBCC CHECKIDENT (DetalleVentas, RESEED,0);
	END
END
GO

--Actualiza el estado de detalle de venta de los artículos depende si la venta se realizó o canceló
CREATE TRIGGER TR_ActualizarEstadoDVA
ON Ventas
AFTER UPDATE
AS
BEGIN
set nocount on;
IF (SELECT Estado_Venta FROM inserted) = 'Realizada'
	BEGIN
	IF EXISTS (SELECT * FROM DetalleVentaArticulos WHERE ID_Venta_DVA= (SELECT ID_Venta FROM inserted))
	UPDATE DetalleVentaArticulos
	SET Estado_DVA = 'Realizado'
	WHERE ID_Venta_DVA = (SELECT ID_Venta FROM inserted);
	DBCC CHECKIDENT (DetalleVentaArticulos, RESEED,0);
	END
ELSE IF (SELECT Estado_Venta FROM inserted) = 'Cancelada'
	BEGIN
	IF EXISTS (SELECT * FROM DetalleVentaArticulos WHERE ID_Venta_DVA= (SELECT ID_Venta FROM inserted))
	UPDATE DetalleVentaArticulos
	SET Estado_DVA = 'Cancelado'
	WHERE ID_Venta_DVA = (SELECT ID_Venta FROM inserted);
	DBCC CHECKIDENT (DetalleVentaArticulos, RESEED,0);
	END
END
GO

--Aplica la promocion si es que la hay(Como es un 2x1, pone el precio del detalle de venta (2) en= 0.00)
CREATE TRIGGER TR_AplicarPromocion
ON Ventas
AFTER UPDATE
AS
BEGIN
set nocount on;
	BEGIN
	IF((SELECT Estado_Venta FROM Ventas WHERE (ID_Venta= (SELECT MAX(ID_Venta) FROM Ventas)))= 'En proceso')
	BEGIN
	IF ((SELECT ID_Promocion_Venta FROM Ventas WHERE (ID_Venta= (SELECT MAX(ID_Venta) FROM Ventas))) != 'NULL')
	UPDATE DetalleVentas
	SET Precio = 0.00
	WHERE ID_Venta_DV= (SELECT MAX(ID_Venta) FROM Ventas) AND ID_DetalleVenta= 2;
	END
	END
END
GO

/* FIN TRIGGERS PARA VISUAL STUDIO */
