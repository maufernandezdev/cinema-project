/*CREACIÓN DE TABLAS*/

create table Peliculas
(
ID_Pelicula char(4) not null,
ID_Estado_Pelicula varchar(20) not null,
Título_Pelicula varchar(50) not null,
Duración_Pelicula int not null,
Clasificación_Pelicula varchar(50) not null,
URL_Portada varchar(50) not null,
constraint PKPeliculas primary key (ID_Pelicula)
)
go

create table Categorias
(
ID_Categoria int identity(1,1) not null,
Nombre_Categoria varchar(30) not null,
constraint PKCategorias primary key (ID_Categoria)
)
go

create table CategoriasxPeliculas
(
ID_Categoria_CatxPelicula int not null,
ID_Pelicula_CatxPelicula char (4) not null,
constraint PKCategoriasxPeliculas primary key (ID_Categoria_CatxPelicula, ID_Pelicula_CatxPelicula),
constraint FKCategoriasxPeliculas foreign key (ID_Categoria_CatxPelicula) references Categorias (ID_Categoria),
constraint FKCategoriasxPeliculas1 foreign key (ID_Pelicula_CatxPelicula) references Peliculas (ID_Pelicula)
)
go

create table Funciones
(
ID_Funcion char(6) not null,
ID_Pelicula_Funcion char(4) not null,
ID_Estado_Funcion varchar(20) not null CHECK(ID_Estado_Funcion = 'Activa' or ID_Estado_Funcion = 'Inactiva') 
constraint FKFunciones foreign key (ID_Pelicula_Funcion) references Peliculas(ID_Pelicula),
constraint PKFunciones primary key (ID_Funcion, ID_Pelicula_Funcion)
)
go

create table Promociones
(
ID_Promocion char(10) not null,
Estado_Promocion varchar(20) not null,
Codigo_Promocion char(8) not null,
Nombre_Promocion varchar(30) not null,
Descripcion_Promocion varchar(50) not null,
Descuento_Promocion decimal(8,2) not null,
Url_imagen varchar(40) null,
constraint PKPromocion primary key (ID_Promocion)
)
go


create table Usuarios
(
DNI_Usuario char(8) not null UNIQUE,
Estado_Usuario varchar(20) not null CHECK(Estado_Usuario = 'Activo'or Estado_Usuario = 'Inactivo') 
Nombre varchar(30) not null,
Apellido varchar(30) not null,
Contraseña char(30) not null,
Fecha_Nac date not null,
Correo varchar(50) not null,
Tipo_Usuario int not null check(Tipo_Usuario = 1 or Tipo_Usuario = 2),
URL_fotoPerfil varchar(50) null,
constraint PKUsuario primary key (DNI_Usuario)
)
go

CREATE TABLE Ventas 
(
ID_Venta int not null IDENTITY,
DNI_Cliente_Venta char(8) not null, 
Estado_Venta varchar(20) not null DEFAULT 'Realizada' CHECK(Estado_Venta = 'Realizada' or Estado_Venta = 'Cancelada' or Estado_Venta='En proceso'),
ID_Promocion_Venta char(10) null, 
Fecha DATE NOT NULL,
Total decimal (8,2),
constraint PKVentas primary key (ID_Venta),
constraint FKVentas foreign key (DNI_Cliente_Venta) references Usuarios(DNI_Usuario),
constraint FKVentas2 foreign key (ID_Promocion_Venta) references Promociones(ID_Promocion)
) 
GO


CREATE TABLE Articulos 
(
ID_Articulo char(4) not null,
Estado_Articulo varchar(20) not null, 
Nombre_Articulo varchar(30) not null, 
Descripción_Articulo varchar(50) not null,
Stock_Articulo int not null,
Precio decimal(8,2) not null,
URL_Articulo varchar(50) not null,
constraint PK_Articulos primary key (ID_Articulo)
)
GO

CREATE TABLE DetalleVentaArticulos 
(
ID_Venta_DVA int not null,
ID_DVA INT NOT NULL IDENTITY, 
ID_Articulo_DVA char(4) not null, 
Estado_DVA varchar(20) not null DEFAULT 'Realizado' CHECK(Estado_DVA = 'Realizado' or Estado_DVA = 'Cancelado' or Estado_DVA = 'En proceso'), 
Cantidad int not null,
Precio decimal (8,2) not null, 
constraint PKDetalleVentaArticulos primary key (ID_Venta_DVA,ID_DVA),
constraint FKDetalleVentaArticulos foreign key (ID_Venta_DVA) references Ventas(ID_Venta),
constraint FKDetalleVentaArticulos1 foreign key (ID_Articulo_DVA) references Articulos(ID_Articulo)
)
GO

create table Sucursales
(
ID_Sucursal char(4) not null,
Estado_Sucursal varchar(20) not null,
Nombre_Sucursal varchar(50) not null,
Dirección_Sucursal varchar(50) not null,
Localidad_Sucursal varchar(50) not null,
Provincia_Sucursal varchar(50) not null,
URL_Sucursal varchar(40) null,
constraint PKSucursales primary key (ID_Sucursal)
)
go

create table TipoSalas
(
ID_TipoSala int identity(1,1) not null,
Descripcion_TipoSala varchar(30) not null,
constraint PKTipoSalas primary key (ID_TipoSala)
)
go

create table Salas
(
ID_Sala char(4) not null,
ID_Sucursal_Sala char(4) not null,
ID_Estado_Sala varchar(20) not null,
ID_TipoSala_Sala int not null,
constraint PKSalas primary key (ID_Sala,ID_Sucursal_Sala),
constraint FKSalas foreign key (ID_Sucursal_Sala) references Sucursales(ID_Sucursal),
constraint FKSalas2 foreign key (ID_TipoSala_Sala) references TipoSalas (ID_TipoSala)
)
go

create table FuncionesxSala
(
ID_Funcion_FuncionxSala char(6) not null,
ID_Pelicula_FuncionxSala char(4) not null,
ID_Sucursal_FuncionxSala char(4) not null,
ID_Sala_FuncionxSala char(4) not null,
ID_Estado_FuncionxSala varchar(20) not null,
Hora_Inicio_FuncionxSala time not null,
Hora_Fin_FuncionxSala time not null,
Fecha_FuncionxSala date not null,
Precio_FuncionxSala decimal(8,2) null,
constraint FKFuncionesxSala1 foreign key (ID_Funcion_FuncionxSala, ID_Pelicula_FuncionxSala) references Funciones(ID_Funcion, ID_Pelicula_Funcion),
constraint FKFuncionesxSala2 foreign key (ID_Sala_FuncionxSala, ID_Sucursal_FuncionxSala) references Salas(ID_Sala, ID_Sucursal_Sala),
constraint PKFuncionesxSala primary key (ID_Funcion_FuncionxSala, ID_Pelicula_FuncionxSala, ID_Sucursal_FuncionxSala, ID_Sala_FuncionxSala,Fecha_FuncionxSala)
)
go


create table Asientos 
(
ID_Asiento char(3) not null, 
ID_Sala_Asiento char(4) not null,
ID_Sucursal_Asiento char(4) not null,
Tipo_Asiento varchar (30) not null,
constraint FKAsientos foreign key (ID_Sala_Asiento, ID_Sucursal_Asiento) references Salas(ID_Sala,ID_Sucursal_Sala),
constraint PKAsientos primary key (ID_Asiento,ID_Sala_Asiento,ID_Sucursal_Asiento)
)
go

create table FuncionesxSalasxAsiento 
(
ID_Funcion_FSA char(6) not null,
ID_Pelicula_FSA char(4) not null,
ID_Sucursal_FSA char(4) not null,
ID_Sala_FSA char(4) not null,
ID_Asiento_FSA char(3) not null, 
Estado_FSA varchar(20) not null,
Fecha_FuncionxSalaAsiento date not null,
constraint FKFxSalaxAsiento foreign key (ID_Funcion_FSA, ID_Pelicula_FSA, ID_Sucursal_FSA, ID_Sala_FSA,Fecha_FuncionxSalaAsiento)
references FuncionesxSala (ID_Funcion_FuncionxSala, ID_Pelicula_FuncionxSala, ID_Sucursal_FuncionxSala, ID_Sala_FuncionxSala,Fecha_FuncionxSala),
constraint FKFxSalaxAsiento2 foreign key (ID_Asiento_FSA,ID_Sala_FSA,ID_Sucursal_FSA)
references Asientos (ID_Asiento,ID_Sala_Asiento,ID_Sucursal_Asiento),
constraint PKFxSalaxAsiento primary key (ID_Funcion_FSA, ID_Pelicula_FSA, ID_Sucursal_FSA, ID_Sala_FSA, ID_Asiento_FSA,Fecha_FuncionxSalaAsiento),
)
GO

CREATE TABLE DetalleVentas
( 
ID_Venta_DV int not null,
ID_DetalleVenta int NOT NULL IDENTITY,
ID_Funcion_DV char(6) not null,
ID_Pelicula_DV char(4) not null, 
ID_Sucursal_DV char(4) not null,
ID_Sala_DV char(4) not null,
ID_Asiento_DV char(3) not null,
Fecha_DV date not null,
Precio decimal (8,2) not null,
Estado_DV varchar(20) not null DEFAULT 'Realizado' CHECK(Estado_DV = 'Realizado'or Estado_DV = 'Cancelado' or Estado_DV ='En proceso'), 
constraint FKDetalleVentas foreign key (ID_Funcion_DV, ID_Pelicula_DV, ID_Sucursal_DV, ID_Sala_DV, ID_Asiento_DV,Fecha_DV) 
references FuncionesxSalasxAsiento (ID_Funcion_FSA, ID_Pelicula_FSA, ID_Sucursal_FSA, ID_Sala_FSA, ID_Asiento_FSA,Fecha_FuncionxSalaAsiento ),
constraint FKDetalleVentas1 foreign key (ID_Venta_DV) references Ventas(ID_Venta),
constraint PKDetalleVentas primary key (ID_DetalleVenta, ID_Venta_DV)
)
go

/*PROCEDIMIENTOS DE ALMACENADO*/

/*ME TRAE TODAS LAS PELICULAS QUE ESTÉN EN FUNCIONESXSALA Y QUE ESTÉN ACTIVAS*/
create procedure sp_pelis
as
select distinct URL_Portada,Título_Pelicula, ID_Pelicula_FuncionxSala from Peliculas
inner join Funciones on ID_Pelicula_Funcion = ID_Pelicula
inner join FuncionesxSala on ID_Funcion_FuncionxSala = ID_Funcion AND ID_Pelicula_FuncionxSala = ID_Pelicula_Funcion
inner join Salas on ID_Sala = ID_Sala_FuncionxSala AND ID_Sucursal_Sala = ID_Sucursal_FuncionxSala
inner join TipoSalas on ID_TipoSala_Sala = ID_TipoSala 
where ID_Estado_FuncionxSala = 'Activa'
go

/*ME TRAE TODAS LAS PELICULAS 2D QUE ESTÉN EN FUNCIONESXSALA Y QUE ESTÉN ACTIVAS*/
create procedure url_2d
as
select distinct URL_Portada,Título_Pelicula, ID_Pelicula_FuncionxSala from Peliculas
inner join Funciones on ID_Pelicula_Funcion = ID_Pelicula
inner join FuncionesxSala on ID_Funcion_FuncionxSala = ID_Funcion AND ID_Pelicula_FuncionxSala = ID_Pelicula_Funcion
inner join Salas on ID_Sala = ID_Sala_FuncionxSala AND ID_Sucursal_Sala = ID_Sucursal_FuncionxSala
inner join TipoSalas on ID_TipoSala_Sala = ID_TipoSala where Descripcion_TipoSala = '2D' and ID_Estado_FuncionxSala = 'Activa'
go


/*ME TRAE TODAS LAS PELICULAS 3D QUE ESTÉN EN FUNCIONESXSALA Y QUE ESTÉN ACTIVAS*/
create procedure url_3d
as
select distinct URL_Portada,Título_Pelicula, ID_Pelicula_FuncionxSala from Peliculas
inner join Funciones on ID_Pelicula_Funcion = ID_Pelicula
inner join FuncionesxSala on ID_Funcion_FuncionxSala = ID_Funcion AND ID_Pelicula_FuncionxSala = ID_Pelicula_Funcion
inner join Salas on ID_Sala = ID_Sala_FuncionxSala AND ID_Sucursal_Sala = ID_Sucursal_FuncionxSala
inner join TipoSalas on ID_TipoSala_Sala = ID_TipoSala where Descripcion_TipoSala = '3D' and ID_Estado_FuncionxSala = 'Activa'
go


/*ME TRAE TODAS LAS PELICULAS 4D QUE ESTÉN EN FUNCIONESXSALA Y QUE ESTÉN ACTIVAS*/
create procedure url_4d
as
select distinct URL_Portada,Título_Pelicula, ID_Pelicula_FuncionxSala from Peliculas
inner join Funciones on ID_Pelicula_Funcion = ID_Pelicula
inner join FuncionesxSala on ID_Funcion_FuncionxSala = ID_Funcion AND ID_Pelicula_FuncionxSala = ID_Pelicula_Funcion
inner join Salas on ID_Sala = ID_Sala_FuncionxSala AND ID_Sucursal_Sala = ID_Sucursal_FuncionxSala
inner join TipoSalas on ID_TipoSala_Sala = ID_TipoSala where Descripcion_TipoSala = '4D' and ID_Estado_FuncionxSala = 'Activa'
go


/*ME TRAE TODAS LAS PELICULAS PREMIUM QUE ESTÉN EN FUNCIONESXSALA Y QUE ESTÉN ACTIVAS*/
create procedure url_PREMIUM
as
select distinct URL_Portada,Título_Pelicula, ID_Pelicula_FuncionxSala from Peliculas
inner join Funciones on ID_Pelicula_Funcion = ID_Pelicula
inner join FuncionesxSala on ID_Funcion_FuncionxSala = ID_Funcion AND ID_Pelicula_FuncionxSala = ID_Pelicula_Funcion
inner join Salas on ID_Sala = ID_Sala_FuncionxSala AND ID_Sucursal_Sala = ID_Sucursal_FuncionxSala
inner join TipoSalas on ID_TipoSala_Sala = ID_TipoSala where Descripcion_TipoSala = 'PREMIUM' and ID_Estado_FuncionxSala = 'Activa'
go

/*TRAE LA SALA Y EL PRECIO DE ESE TIPO DE SALA*/

create procedure spSalas
as
select distinct Descripcion_TipoSala, Precio_FuncionxSala from TipoSalas
inner join Salas on ID_TipoSala_Sala = ID_TipoSala
inner join FuncionesxSala on ID_Sala_FuncionxSala = ID_Sala
go


/*TRAE LAS FACTURAS DE UN CLIENTE DETERMINADO*/

create procedure spVentaCliente
(
@Correo varchar(50), @Contraseña char(30)
)
as
begin
select ID_Venta, Fecha, Total, ID_Promocion_Venta, Estado_Venta from Ventas
inner join Usuarios on DNI_Usuario = DNI_Cliente_Venta
where Correo = @Correo and Contraseña = @Contraseña and Estado_Venta='Realizada'
end
go


/*TRAE LOS DATOS DE LOS ARTICULOS*/
create procedure spArticulos
as
select Nombre_Articulo, Descripción_Articulo, Precio, URL_Articulo from Articulos
go


/*TRAE LA SUMA DE UN DETALLE DE VENTA DE ARTICULOS CON EL DNI QUE LE ENVIEMOS*/
create procedure sp_totalDva_dni
(@dni char(8))
as 
begin 
SELECT SUM(Precio*Cantidad)[Total]From DetalleVentaArticulos dva
INNER JOIN Ventas v on v.ID_Venta =dva.ID_Venta_DVA
where v.DNI_Cliente_Venta=@dni
end
go

/*TRAE LA SUMA DE UN DETALLE DE VENTA CON EL NRO DE VENTA QUE LE ENVIEMOS*/
create procedure sp_totalDetalleVenta_nv
(@id int)
as 
begin 
SELECT SUM(Precio)[Total]From DetalleVentas dv
INNER JOIN Ventas v on v.ID_Venta=dv.ID_Venta_DV
where v.ID_Venta =@id
end
go

-- TRAE EL TOTAL DE LA VENTA POR NUMERO DE VENTA -- 
create procedure sp_totalventa_NroVenta
(@id int)
as 
begin 
SELECT SUM(v.Total)as[Total]From Ventas v
where v.ID_Venta =@id
end
go

/*BAJA LÓGICA DE UNA VENTA*/
CREATE PROCEDURE sp_BajaLogicaVenta
(@id int,@estado varchar(20)='Cancelada')
AS
BEGIN
UPDATE Ventas
SET
Estado_Venta=@estado
WHERE ID_Venta=@id
END
RETURN
GO

/*BAJA LÓGICA DE UN DETALLE VENTA ARTÍCULO DETERMINADO*/
CREATE PROCEDURE sp_BajaLogicaDetalleVentaArt
(@id int,@estado varchar(20)='Cancelado')
AS
BEGIN
UPDATE DetalleVentaArticulos
SET
Estado_DVA=@estado
WHERE ID_Venta_DVA=@id
END
RETURN
GO

/*BAJA LÓGICA DE UN DETALLE VENTA DETERMINADO*/
CREATE PROCEDURE sp_BajaLogicaDetalleVenta
(@id int,@estado varchar(20)='Cancelado')
AS
BEGIN
UPDATE DetalleVentas
SET
Estado_DV=@estado
WHERE ID_Venta_DV=@id
END
RETURN
GO

/*BAJA LÓGICA DE UN CLIENTE*/
CREATE PROCEDURE sp_bajaCliente
(@Correo varchar(50), @Contraseña char(30),@estado varchar(20)='Inactivo')
AS
BEGIN
update Usuarios
set Estado_Usuario=@estado where Correo=@Correo and Contraseña=@Contraseña
END
GO

/*ALTA DE UN CLIENTE*/
CREATE PROCEDURE sp_altaCliente
(@Correo varchar(50), @Contraseña char(30), @estado varchar(20)='Activo')
AS
BEGIN
UPDATE Usuarios
SET
Estado_Usuario = @estado
WHERE Correo = @Correo AND Contraseña = @Contraseña
END
GO

--baja un detalle de venta de articulo
CREATE PROCEDURE sp_bajaDetalleVentasArts
(@id int,@id_dva int,@estado varchar(20)='Cancelado')
AS
BEGIN
UPDATE DetalleVentaArticulos
SET
Estado_DVA=@estado
WHERE ID_Venta_DVA=@id AND ID_DVA=id_dva
END
RETURN
GO

--baja un detalle de venta
CREATE PROCEDURE sp_bajaDetalleVentas
(@id int,@id_dv int,@estado varchar(20)='Cancelado')
AS
BEGIN
UPDATE DetalleVentas
SET
Estado_DV=@estado
WHERE ID_Venta_DV=@id AND ID_DetalleVenta=@id_dv
END
GO

-- TRA EL TOTAL DE UN DETALLE DE VENTA DE ARTICULO -- PARAMETROS: ID VENTA ID DETALLE DE VENTA --
CREATE PROCEDURE sp_precioxcant_dva
(@id int, @id_dva int)
AS
BEGIN
SELECT Cantidad*Precio From DetalleVentaArticulos
WHERE ID_Venta_DVA=@id AND ID_DVA=@id_dva
END 
GO


-- CUANDO SE CANCELA UN DETALLA DE VENTA , SE LE RESTA A LA VENTA ESE MONTA 
CREATE PROCEDURE sp_restarDetalles
(@id_venta int,@monto decimal)
AS
BEGIN 
UPDATE Ventas
SET 
Total=Total-@monto
WHERE ID_Venta=@id_venta
END 
GO 

--TRAE UN DETALLE DE VENTA POR NÚMERO DE VENTA
CREATE PROCEDURE sp_detalleVenta_nv
(@id int)
AS
BEGIN
Select ID_Venta_DV[ID Venta],ID_DetalleVenta[ID Detalle],
ID_Funcion_DV[Función],ID_Pelicula_DV[Película],ID_Sala_DV[Sala],ID_Asiento_DV[Asiento],
Fecha_DV[Fecha],Precio,Estado_DV from DetalleVentas WHERE Estado_DV='Realizado' AND ID_Venta_DV=@id
END 
GO

--TRAE UN DETALLE DE VENTA DE ARTICULO POR NÚMERO DE VENTA
CREATE PROCEDURE sp_detalleVenArt_nv
(@id int)
AS
BEGIN
Select ID_Venta_DVA[ID Venta],ID_DVA[ID Detalle],
ID_Articulo_DVA[ID artículo],Cantidad,Precio
from DetalleVentaArticulos WHERE Estado_DVA='Realizado' AND ID_Venta_DVA=@id
END 
GO


/* TRAE LAS SUMA DE UN DETALLE DE VENTA DE ARTICULOS CON EL DNI Y EL NUMERO DE VENTA QUE LE ENVIEMOS*/
create procedure sp_totalDva_dninv
(@dni char(8),@nro_venta int)
AS 
BEGIN 
SELECT SUM(Precio*Cantidad)as[Total]From DetalleVentaArticulos dva
INNER JOIN Ventas v on v.ID_Venta =dva.ID_Venta_DVA
WHERE v.DNI_Cliente_Venta=@dni AND dva.ID_Venta_DVA=@nro_venta
END
GO

-- TRAE EL TOTAL DE UN DETALLE DE VENTA POR DNI
CREATE PROCEDURE sp_totalDv_dni 
(@dni char(8))
as 
begin 
SELECT SUM(dv.Precio)[Total]From DetalleVentas dv
INNER JOIN Ventas v on v.ID_Venta =dv.ID_Venta_DV
where v.DNI_Cliente_Venta='32836060'
end
go


/*AGREGA UN USUARIO*/
create procedure spAgregarUsuario
(
@Nombre varchar(40), @Apellido varchar(40), @Dni char(8), @Contraseña char(30), @Correo varchar(30), @Fecha date
)
as
insert into Usuarios
select @Dni, 'Activo', @Nombre, @Apellido, @Contraseña, @Fecha, @Correo, 1,NULL
go

/*VERIFICA SI EXISTE EL USUARIO CON EL CORREO Y LA CONTRASEÑA*/

create procedure spVerificarUsuario
(
@Correo varchar(30), @Contraseña char(30)
)
as
begin
select * from Usuarios where Correo = @Correo AND Contraseña = @Contraseña 
end
go

/*TRAE EL PERFIL DE UN ADMINISTRADOR DETERMINADO*/

create procedure spTraerAdmin
(
@Correo varchar(30), @Contraseña char(30)
)
as
begin
select Nombre, Apellido, Fecha_Nac, Correo, URL_fotoPerfil from Usuarios
where Correo = @Correo and Contraseña = @Contraseña and Tipo_Usuario = 2
end
go

/*ACTUALIZA EL CORREO*/

create procedure spActualizarCorreo
(
@Correo varchar(30)
)
as
begin
update Usuarios set Correo = @Correo
end
go

/*ACTUALIZA LA CONTRASEÑA*/

create procedure spActualizarContraseña
(
@Contraseña char(30)
)
as
begin
update Usuarios set Contraseña = @Contraseña
end
go

/*DEVUELVE LOS DATOS DE PROMOCIONES*/
CREATE PROCEDURE Sp_Promos 
AS
BEGIN
SELECT p.Nombre_Promocion,p.Descripcion_Promocion,p.Url_imagen FROM Promociones p
END
GO


/*TRAE CIERTOS CAMPOS DE FUNCIONESXSALA DEPENDIENDO EL ID_PELICULA, ID_SUCURSAL E ID_TIPOSALA*/
CREATE PROCEDURE spSucursalesFuncion
(
@ID_Pelicula char(4), @ID_Sucursal char(4), @ID_TipoSala int
)
AS
BEGIN
SELECT Hora_Inicio_FuncionxSala, Fecha_FuncionxSala, Precio_FuncionxSala FROM FuncionesxSala
INNER JOIN Salas ON ID_Sala = ID_Sala_FuncionxSala
INNER JOIN TipoSalas ON ID_TipoSala = ID_TipoSala_Sala
WHERE ID_Pelicula_FuncionxSala = @ID_Pelicula 
AND ID_Sucursal_FuncionxSala = @ID_Sucursal AND ID_TipoSala = @ID_TipoSala AND ID_Estado_FuncionxSala = 'Activa'
GROUP BY Hora_Inicio_FuncionxSala, Fecha_FuncionxSala, Precio_FuncionxSala
ORDER BY Fecha_FuncionxSala
END
GO



/*DEVUELVE LOS CAMPOS DE UNA FUNCION DEPENDIENDO EL ID_SUCURSAL E ID_PELICULA*/
create procedure sp_Horarios_Fechas
(
@ID_Sucursal char(4), @ID_Pelicula char(4)
)
as
begin
select * from FuncionesxSala
where ID_Sucursal_FuncionxSala = @ID_Sucursal AND ID_Pelicula_FuncionxSala = @ID_Pelicula
order by convert(date,Fecha_FuncionxSala)
end
go

/*TRAE EL TOP 3 DE PELICULAS MAS VENDIDAS*/

CREATE PROCEDURE sp_top3
AS 
BEGIN
Select TOP 3 p.Título_Pelicula,SUM(dv.Precio)AS[Recaudación], p.URL_Portada from Peliculas p
INNER JOIN DetalleVentas dv on dv.ID_Pelicula_DV=p.ID_Pelicula
GROUP BY dv.ID_Pelicula_DV,p.Título_Pelicula,p.Duración_Pelicula,p.URL_Portada
ORDER BY COUNT(dv.ID_Pelicula_DV) DESC
END
GO


/*SUCURSAL DEL MES POR SUMA DE RECAUDACION*/
CREATE PROCEDURE sp_mejorSucuJunio
AS
BEGIN
Select top 1 s.Nombre_Sucursal, SUM(v.Total) as [Total] from Sucursales s
INNER JOIN DetalleVentas dv on dv.ID_Sucursal_DV=s.ID_Sucursal
INNER JOIN Ventas v on v.ID_Venta=dv.ID_Venta_DV
WHERE V.Fecha between '2020-06-01' AND '2020-06-30'
GROUP BY s.Nombre_Sucursal,s.ID_Sucursal
ORDER BY Total DESC
END
GO


/*TRAE EL TOTAL DE LA VENTA DIARIA*/
CREATE PROCEDURE sp_TotalVentaDiaria
AS
BEGIN
SELECT SUM(v.Total) AS [Recaudación diaria] From Ventas v
where v.Fecha = (SELECT CONVERT(VARCHAR(10), GETDATE(), 120))
END 
GO


/*TRAE EL TOTAL DE LA VENTA MENSUAL*/
CREATE PROCEDURE sp_TotalVentaMensual
AS
BEGIN
SELECT SUM(v.Total) AS [Recaudación mensual] From Ventas v
where v.Fecha between '2020-06-01' AND '2020-06-30'
END 
GO

/*TRAE EL TOTAL DE LA VENTA ANUAL*/
CREATE PROCEDURE sp_TotalVentaAnual
AS
BEGIN
SELECT SUM(v.Total) AS [Recaudación anual] From Ventas v
where v.Fecha between '2020-01-01' and '2020-12-31'
END 
GO

/*TRAE EL TOTAL DE CLIENTES ACTIVOS E INACTIVOS*/
CREATE PROCEDURE sp_ActividadUsuarios
AS
BEGIN
SELECT c.Estado_Usuario AS [Estado],count(c.Estado_Usuario) AS [Total] FROM Usuarios c where c.Tipo_Usuario = 1
GROUP BY c.Estado_Usuario
ORDER BY Total DESC
END 
GO

/*TRAE LA CANTIDAD DE CLIENTES ACTIVOS*/
CREATE PROCEDURE sp_ClientesActivos
AS
BEGIN
SELECT c.Estado_Usuario AS [Estado],count(c.Estado_Usuario) AS [Total] FROM Usuarios c
WHERE c.Estado_Usuario='Activo'
GROUP BY c.Estado_Usuario
END 
GO


/*TRAE LA CANTIDAD DE CLIENTES INACTIVOS*/
CREATE PROCEDURE sp_ClientesInactivos
AS
BEGIN
SELECT c.Estado_Usuario AS [Estado],count(c.Estado_Usuario) AS [Total] FROM Usuarios c
WHERE Estado_Usuario='Inactivo'
GROUP BY c.Estado_Usuario
END 
GO


/*TRAE LA PROMOCION MÁS USADA EN JUNIO*/
CREATE PROCEDURE sp_mejorPromoJunio
AS
BEGIN
Select top 1 p.Nombre_Promocion AS [Promoción] From Promociones p
INNER JOIN Ventas v on v.ID_Promocion_Venta=p.ID_Promocion
WHERE v.Fecha between '2020-06-01' AND '2020-06-30'
GROUP BY p.Nombre_Promocion
ORDER BY COUNT(v.ID_Promocion_Venta) DESC
END 
GO

/* sp listado ventas */
/*1 parametro*/
-- VENTAS POR FECHA
CREATE PROCEDURE sp_ventas_Fecha
(@fecha date)
AS
begin 
select * from Ventas v
where v.Fecha=@fecha
end 
go


/* 2 parametros */
CREATE PROCEDURE sp_ventas_DniNroVenta
(@dni char(8),@id int)
AS
BEGIN
Select * From Ventas v 
WHERE v.DNI_Cliente_Venta=@dni AND V.ID_Venta=@id
END 
GO

-- TRAE LAS VENTAS POR DNI Y FECHA -- 
CREATE PROCEDURE sp_ventas_DniFecha
(@dni char(8),@fecha date)
AS
BEGIN
SELECT * FROM Ventas v
WHERE v.DNI_Cliente_Venta=@dni AND v.Fecha=@fecha
END 
GO

-- TRAE LAS VENTAS POR NUMERO DE VENTA Y FECHA --
CREATE PROCEDURE sp_ventas_NroVentaFecha
(@id int,@fecha date)
AS
BEGIN
SELECT * FROM Ventas v
WHERE v.ID_Venta=@id AND v.Fecha=@fecha
END 
GO

-- TRAE LAS VENTAS POR DNI NUMERO DE VENTA Y FECHA --
CREATE PROCEDURE sp_ventas_DniNroVentaFecha
(@dni char(8),@id int,@fecha date)
AS
BEGIN
SELECT * FROM Ventas v
WHERE v.DNI_Cliente_Venta=@dni AND v.ID_Venta=@id AND v.Fecha=@fecha
END 
GO

-- agrega un registro pelicula -- 
CREATE PROCEDURE sp_AgregarPelicula
(@id_pelicula char(4),
@estado varchar(20),
@titulo varchar(50),
@duracion int,
@clasif varchar(50),
@url varchar(50)
)
AS
BEGIN 
INSERT INTO Peliculas 
SELECT @id_pelicula,@estado,@titulo,@duracion, @clasif,@url
END
GO 

-- actualiza un registro pelicula -- 
CREATE PROCEDURE sp_actualizarPelicula
(@id_pelicula char(4),
@estado varchar(20),
@titulo varchar(50),
@duracion int, 
@clasif varchar(50),
@url varchar(50)
)
AS
BEGIN
UPDATE Peliculas
SET
ID_Pelicula=@id_pelicula,
ID_Estado_Pelicula=@estado,
Título_Pelicula=@titulo,
Duración_Pelicula=@duracion,
Clasificación_Pelicula=@clasif,
URL_Portada=@url
WHERE ID_Pelicula=@id_pelicula
END
RETURN
GO

    
-- eliminar un registro pelicula por id  de pelicula --  
CREATE PROCEDURE sp_deletePelicula
(
@id_pelicula char(4)
)
as
BEGIN
DELETE FROM Peliculas
WHERE ID_Pelicula=@id_pelicula
END
GO

 
--agrega un registro articulo -- 
CREATE PROCEDURE sp_AgregarArticulos
(@id_articulo char(4),
@estado varchar(20),
@nombre varchar(30),
@descripcion varchar(50),
@stock int,
@precio decimal(8,2),
@url varchar(50)
)
AS
BEGIN 
INSERT INTO Articulos 
SELECT @id_articulo,@estado,@nombre,@descripcion, @stock, @precio, @url
END
GO 

-- actualiza un registro articulo--
CREATE PROCEDURE sp_actualizarArticulo
(@id_articulo char(4),
@estado varchar(20),
@nombre varchar(30),
@descripcion varchar(50),
@stock int,
@precio decimal(8,2),
@url varchar(50)
)
AS
BEGIN
UPDATE Articulos
SET
ID_Articulo=@id_articulo,
Estado_Articulo=@estado,
Nombre_Articulo=@nombre,
Descripción_Articulo=@descripcion,
Stock_Articulo=@stock,
Precio=@precio,
URL_Articulo=@url
WHERE ID_Articulo=@id_articulo
END
RETURN
GO

-- eliminar un registro pelicula por id  de pelicula --
CREATE PROCEDURE sp_deleteArticulo
(
@id_articulo char(4)
)
as
BEGIN
DELETE FROM Articulos
WHERE ID_Articulo=@id_articulo
END
GO



-- TRAE EL CLIENTE QUE DINERO GASTO EN VENTAS , DEL MES DE JUNIO... 
CREATE PROCEDURE sp_clienteDelMes
AS
BEGIN
SELECT top 1 CONCAT(u.Nombre,' ',u.Apellido) as [Cliente], u.Correo From Usuarios u
INNER JOIN Ventas v on v.DNI_Cliente_Venta=u.DNI_Usuario
WHERE v.Fecha BETWEEN '2020-06-01' AND '2020-06-30'
GROUP BY CONCAT(u.Nombre,' ',u.Apellido),u.Correo
ORDER BY SUM(v.Total) DESC
END
GO

/*TRAE LOS DETALLES DE FACTURA DE UN ID DE VENTA DETERMINADO*/
create procedure spDetFacturaArt
(
@ID INT
)
as
begin
select Nombre_Articulo,Descripción_Articulo,Cantidad, DetalleVentaArticulos.Precio from DetalleVentaArticulos 
inner join Articulos on ID_Articulo = ID_Articulo_DVA where ID_Venta_DVA = @ID
end
go

--Asientos disponibles por funcion, sala y sucursal
CREATE PROCEDURE SP_AsientosDisponible
@ID_Pelicula as char(6),
@ID_Sucursal as char (4),
@Horario as varchar(28),
@Fecha as varchar(28)
AS
DECLARE @ID_Funcion char(6);
DECLARE @ID_Sala char(4);
SELECT @ID_Funcion = ID_Funcion_FuncionxSala, @ID_Sala = ID_Sala_FuncionxSala 
FROM FuncionesxSala 
WHERE (ID_Pelicula_FuncionxSala = @ID_Pelicula) AND (ID_Sucursal_FuncionxSala = @ID_Sucursal)  AND (Hora_Inicio_FuncionxSala = (SELECT CONVERT(time, @Horario))) 
		AND (Fecha_FuncionxSala= (SELECT CONVERT(date, @Fecha)))
IF exists (SELECT ID_Funcion_FuncionxSala, ID_Sucursal_FuncionxSala, ID_Sala_FuncionxSala, Hora_Inicio_FuncionxSala, Fecha_FuncionxSala 
			FROM FuncionesxSala WHERE (ID_Pelicula_FuncionxSala = @ID_Pelicula) AND (ID_Sucursal_FuncionxSala = @ID_Sucursal)  AND 
			(Hora_Inicio_FuncionxSala = (SELECT CONVERT(time, @Horario))) AND (Fecha_FuncionxSala= (SELECT CONVERT(date, @Fecha))))
BEGIN
SELECT ID_Asiento_FSA
FROM FuncionesxSalasxAsiento
WHERE (ID_Funcion_FSA= @ID_Funcion) AND (ID_Pelicula_FSA= @ID_Pelicula) AND (ID_Sucursal_FSA= @ID_Sucursal) AND (ID_Sala_FSA= @ID_Sala)
		AND (Fecha_FuncionxSalaAsiento= (SELECT CONVERT(date, @Fecha))) AND (Estado_FSA= 'Disponible') 
ORDER BY ID_Asiento_FSA ASC
END
ELSE
PRINT 'NO EXISTE LA FUNCIÓN'
GO


--Selecciona los asientos del cliente, cambiando su estado 'Disponible' a 'Reservado' una vez que finalice la compra pasarán a 'Ocupado'
CREATE PROCEDURE SP_SeleccionarAsiento
(
@ID_Pelicula char(4),
@ID_Sucursal char(4),
@ID_Asiento char (3),
@Horario varchar(28),
@Fecha varchar(28)
)
AS
DECLARE @ID_Funcion char(6);
DECLARE @ID_Sala char(4);
SELECT @ID_Funcion = ID_Funcion_FuncionxSala, @ID_Sala = ID_Sala_FuncionxSala 
FROM FuncionesxSala 
WHERE (ID_Pelicula_FuncionxSala = @ID_Pelicula) AND (ID_Sucursal_FuncionxSala = @ID_Sucursal)  AND (Hora_Inicio_FuncionxSala = (SELECT CONVERT(time, @Horario))) 
		AND (Fecha_FuncionxSala= (SELECT CONVERT(date, @Fecha)))
UPDATE FuncionesxSalasxAsiento SET Estado_FSA='Reservado' 
WHERE (ID_Funcion_FSA = @ID_Funcion) AND (ID_Pelicula_FSA = @ID_Pelicula) AND (ID_Sucursal_FSA = @ID_Sucursal) AND(ID_Sala_FSA = @ID_Sala) AND (ID_Asiento_FSA= @ID_Asiento)
		AND (Fecha_FuncionxSalaAsiento= (SELECT CONVERT(date, @Fecha))) AND (Estado_FSA= 'Disponible')
RETURN
GO

/*--Asientos reservados que se irán mostrando en el resumen de selección (Al momento de seleccionar el cliente un asiento pasa a estar Reservado, 
una vez que hace la compra pasará a 'Ocupado', si cancela la compra vuelve a estar Disponible)*/
CREATE PROCEDURE SP_AsientosReservado
AS
SELECT ID_Asiento_FSA AS 'ID Asiento'
FROM FuncionesxSalasxAsiento 
WHERE (Estado_FSA = 'Reservado')
GO

--Elimina los asientos cuando el cliente se arrepiente de haberlo seleccionado, pasará su estado de estar 'Reservado' a 'Disponible' de nuevo
CREATE PROCEDURE SP_QuitarAsientoSeleccionado
(
@ID_Asiento char(3)
)
AS
UPDATE FuncionesxSalasxAsiento SET Estado_FSA='Disponible' 
WHERE (ID_Asiento_FSA= @ID_Asiento) AND (Estado_FSA='Reservado')
GO

--Se vacían las reservas(Pasan de estado'Reservado' a 'Disponible' nuevamente) si es que el cliente anterior cerró la página sin quitar sus asientos reservados
CREATE PROCEDURE SP_VaciarReservas
AS
UPDATE FuncionesxSalasxAsiento SET Estado_FSA='Disponible' 
WHERE (Estado_FSA = 'Reservado')
SELECT 1
GO

--Devuvelve los articulos disponibles
CREATE PROCEDURE SP_ArticulosDisponible
AS
SELECT ID_Articulo, Nombre_Articulo, Descripción_Articulo AS 'Descripcion_Articulo', Stock_Articulo, Precio, URL_Articulo  
FROM Articulos
WHERE Estado_Articulo = 'Activo'
RETURN
GO
 

 --Chequeea si el código ingresado está asociado a la promoción
CREATE PROCEDURE SP_ChequearPromocion
(
@ID_Promocion char(10),
@Codigo char(8)
)
AS
IF EXISTS (SELECT * FROM Promociones WHERE ID_Promocion= @ID_Promocion AND Codigo_Promocion = @Codigo)
SELECT 1
ELSE
SELECT 0
GO


--Cargar venta en estado 'En proceso' hasta que luego se actualice a 'Realizada' o 'Cancelada'
CREATE PROCEDURE SP_ProcesarVenta
(
@Correo_Cliente varchar(50),
@ID_Promocion char(10)
)
AS
DECLARE @DNI_Cliente char(8);
SELECT @DNI_Cliente = DNI_Usuario FROM Usuarios WHERE (Correo = @Correo_Cliente)
IF(@ID_Promocion= 'sinpromo')
BEGIN
INSERT INTO Ventas(DNI_Cliente_Venta, Estado_Venta, Fecha, Total) 
VALUES (@DNI_Cliente, 'En proceso', (SELECT CONVERT(date, GETDATE())), 0.00)
END
ELSE
INSERT INTO Ventas(DNI_Cliente_Venta, Estado_Venta, ID_Promocion_Venta, Fecha, Total) 
VALUES (@DNI_Cliente, 'En proceso', @ID_Promocion, (SELECT CONVERT(date, GETDATE())), 0.00)
GO

--Cargar detalle de venta en estado 'En proceso' hasta que luego se actualice a 'Realizada' o 'Cancelada'
CREATE PROCEDURE SP_ProcesarDetalleVentas
(
@ID_Funcion char(6),
@ID_Pelicula char(4),
@ID_Sucursal char(4),
@ID_Sala char(4),
@ID_Asiento char (3),
@Fecha varchar(28),
@Precio decimal(8,2)
)
AS
DECLARE @ID_Venta int;
SELECT @ID_Venta= (SELECT MAX(ID_Venta) FROM Ventas)
INSERT INTO DetalleVentas(ID_Venta_DV, ID_Funcion_DV, ID_Pelicula_DV, ID_Sucursal_DV, ID_Sala_DV, ID_Asiento_DV, Fecha_DV, Precio, Estado_DV)
VALUES(@ID_Venta, @ID_Funcion, @ID_Pelicula, @ID_Sucursal, @ID_Sala, @ID_Asiento, (SELECT CONVERT(date, @Fecha)), @Precio, 'En proceso')
GO

--Carga detalle de articulo en estado 'En proceso' hasta que luego se actualice a 'Realizada' o 'Cancelada'
CREATE PROCEDURE SP_ProcesarDetalleVentaArticulos
(
@ID_Articulo char(4),
@Cantidad int,
@Precio decimal(8,2)
)
AS
DECLARE @ID_Venta int;
SELECT @ID_Venta= (SELECT MAX(ID_Venta) FROM Ventas)
INSERT INTO DetalleVentaArticulos (ID_Venta_DVA, ID_Articulo_DVA, Estado_DVA, Cantidad, Precio)
VALUES (@ID_Venta, @ID_Articulo, 'En proceso', @Cantidad, @Precio)
GO

--Finalizar Venta, si se hizo la venta su estado pasará a 'Realizada' y en caso de haber sido cancelada su estado pasará a 'Cancelada'
CREATE PROCEDURE SP_FinalizarVenta
(
@Estado varchar(20)
)
AS
UPDATE Ventas
SET Estado_Venta= @Estado
WHERE  Estado_Venta= 'En proceso'
SELECT 1
GO

--Cancela la venta anterior en caso de que no haya sido finalizada manualmente
CREATE PROCEDURE SP_FinalizarVentaPendiente
AS
IF EXISTS (SELECT * FROM Ventas WHERE (Estado_Venta = 'En proceso') AND ID_Venta= (SELECT MAX(ID_Venta) FROM Ventas))
BEGIN
UPDATE Ventas SET Estado_Venta='Cancelada' 
WHERE (Estado_Venta = 'En proceso') AND ID_Venta= (SELECT MAX(ID_Venta) FROM Ventas)
SELECT 1
END
ELSE
SELECT 0
GO


--Disminuye el stock de los productos comprados
CREATE PROCEDURE SP_DisminuirStock
(
@ID_Articulo char(4),
@Cantidad int
)
AS
UPDATE Articulos SET Stock_Articulo= Stock_Articulo - @Cantidad
WHERE ID_Articulo= @ID_Articulo
SELECT 1
GO

--Una vez finalizada la compra cambia el estado de los asientos comprados de 'Reservado' a 'Ocupado' 
CREATE PROCEDURE SP_ActualizarEstadoAsientos 
AS
UPDATE FuncionesxSalasxAsiento SET Estado_FSA='Ocupado' 
WHERE (Estado_FSA = 'Reservado')
SELECT 1
GO

-- APLICA UNA PROMO DE 2X1 A UN DETALLE DE VENTA -- CADA VENTA PARA NO LA COBRA
CREATE PROCEDURE sp_DescontarPromoDosxUno
(@id_venta int)
AS 
BEGIN
UPDATE DetalleVentas
SET Precio=0.00
WHERE ID_Venta_DV=@id_venta AND ID_DetalleVenta%2=0
END 
GO 

-- APLICA UNA PROMO DE 50% A UN DETALLE DE VENTA -- CADA ENTRADA LA COBRA A LA MITAD
CREATE PROCEDURE sp_DescontarPromoCincuenta
(@id_venta int)
AS 
BEGIN
UPDATE DetalleVentas
SET Precio=Precio*0.5
WHERE ID_Venta_DV=@id_venta 
END 
GO

/*----------------FIN PROCEDIMIENTOS DE ALMACENADO------------------*/

/*------------------------------TRIGGERS---------------------------*/

 
-- SUMA CADA VENTA DE ENTRADA QUE SE REGISTRE EN UN DETALLE DE VENTA , AL TOTAL DE LA VENTA -- 
CREATE TRIGGER tr_Suma_DetalleVenta
on DetalleVentas
after INSERT
AS 
BEGIN 
set nocount on;
UPDATE Ventas
SET Total=Total+(SELECT Precio FROM INSERTED where ID_Venta_DV=ID_Venta)
where ID_Venta in (SELECT ID_Venta_DV from inserted)
END
GO 

-- SUMA CADA VENTA DE ARTICULOS QUE SE REGISTRE EN UN DETALLE DE VENTA , AL TOTAL DE LA VENTA -- 
CREATE TRIGGER tr_Suma_DetalleVentaArticulo
on DetalleVentaArticulos
after INSERT
AS 
BEGIN 
set nocount on;
UPDATE Ventas
SET Total=Total+(SELECT Cantidad*Precio FROM INSERTED where ID_Venta_DVA=ID_Venta)
where ID_Venta in (SELECT ID_Venta_DVA from inserted)
END
GO 

-- CUANDO SE VENDE UN ASIENTO EN UNA ENTRADA , EN FUNCIONES X SALA X ASIENTO CAMBIA EL ESTADO A OCUPADO -- 
CREATE TRIGGER tr_CambiarEstadoAsientos
on DetalleVentas
AFTER INSERT 
AS
BEGIN
set nocount on;
DECLARE
@id_funcion char(6),@id_pelicula char(4),@id_sucursal char(4),@id_sala char(4),@asiento char(3),@fecha date
SELECT @id_funcion= ID_Funcion_DV,@id_pelicula = ID_Pelicula_DV, @id_sucursal = ID_Sucursal_DV,
@id_sala = ID_Sala_DV, @asiento = ID_Asiento_DV,@fecha=Fecha_DV FROM INSERTED
UPDATE FuncionesxSalasxAsiento
SET Estado_FSA='Ocupado'
WHERE ID_Funcion_FSA=@id_funcion AND ID_Pelicula_FSA=@id_pelicula AND ID_Sucursal_FSA=@id_sucursal AND ID_Sala_FSA=@id_sala
AND ID_Asiento_FSA=@asiento AND Fecha_FuncionxSalaAsiento=@fecha
END 
GO

/*------------------------------FIN TRIGGERS---------------------------*/

